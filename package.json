{
  "name": "code-buster",
  "private": true,
  "scripts": {
    "build:app": "yarn --cwd packages/app build",
    "start:app": "yarn --cwd packages/app start",
    "start:api": "yarn --cwd packages/api start:dev",
    "start": "concurrently \"yarn start:app\" \"yarn start:api\""
  },
  "devDependencies": {
    "concurrently": "^5.0.0"
  }
}
